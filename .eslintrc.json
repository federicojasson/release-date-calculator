{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "es6": true
  },
  "globals": {
    "angular": true
  },
  "rules": {
    "camelcase": "error",
    "comma-dangle": "error",
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "no-console": "off",
    "no-use-before-define": [
      "error",
      {
        "functions": false
      }
    ],
    "no-var": "error",
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}
