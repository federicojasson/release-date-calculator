{
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "jasmine": true,
    "node": true
  },
  "rules": {
    "camelcase": "error",
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-use-before-define": "error",
    "quotes": [
      "error",
      "single"
    ]
  }
}
